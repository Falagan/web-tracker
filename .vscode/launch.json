{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Env Go Application",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "env": {
        "WEB_TRACKER_APP_ENV": "development",
        "WEB_TRACKER_SERVER_ADDRESS": "0.0.0.0",
        "WEB_TRACKER_SERVER_PORT": "4001",
        "WEB_TRACKER_SERVER_READ_TIMEOUT": "30",
        "WEB_TRACKER_SERVER_WRITE_TIMEOUT": "30",
        "WEB_TRACKER_SERVER_IDLE_TIMEOUT": "30",
        "WEB_TRACKER_BLOOM_EXPECTED_ELEMENTS": "1000",
        "WEB_TRACKER_BLOOM_FALSE_POSITIVE_RATE": "0.01"
      },
      "args": []
    }
  ]
}
